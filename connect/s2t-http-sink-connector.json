{
    "name": "s2t-http-sink-connector",
    "config": {
        "connector.class": "com.github.castorm.kafka.connect.http.HTTPSinkConnector",
        "tasks.max": "1",
        "http.request.url": "http://app:3030/api/process",
        "http.request.method": "POST",
        "http.request.headers": "Content-Type: application/json",
        "http.timer.interval.millis": "30000",
        "http.timer.catchup.interval.millis": "1000",
        "http.request.body": "{\"id\": \"${id}\", \"value\": \"${value}\"}",
        "http.request.body.format": "json",
        "http.request.body.format.json.schema": "{\"type\": \"object\", \"properties\": {\"id\": {\"type\": \"string\"}, \"value\": {\"type\": \"string\"}}}",
        "http.request.body.format.json.schema.version": "1",
        "http.request.body.format.json.schema.id": "1",
    }
}